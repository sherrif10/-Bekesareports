<?xml version="1.0" encoding="UTF-8"?>

<module configVersion="1.2">
	
	<!-- Base Module Properties -->
	<id>bekesareports</id>
	<name>Bekesareports</name>
	<version>1.0.0-SNAPSHOT</version>
	<package>org.openmrs.module.bekesareports</package>
	<author>sharif</author>
	<description>
			OpenMRS module for Packaging BEKESA Reports powered by the Reporting Module
	</description>

	<activator>org.openmrs.module.bekesareports.BekesareportsActivator</activator>
	

	<require_version>2.1.1</require_version>

	<require_modules>
		<require_module version="1.19.0">
			org.openmrs.module.reporting
		</require_module>
		<require_module version="3.0.0-SNAPSHOT">
			org.openmrs.module.cohort
		</require_module>
	</require_modules>

	<aware_of_modules>
		<aware_of_module>org.openmrs.module.legacyui</aware_of_module>
	</aware_of_modules>


	<!-- Required Global Properties 
	<globalProperty>
		<property>bekesareports.someProperty</property>
